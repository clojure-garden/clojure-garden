{
  "name": "@garden.clojure/frontend",
  "scripts": {
    "cache:clean": "rm -rf .cpcache .shadow-cljs target",
    "assets:clean": "npm run cache:clean && rm -rf ./public/assets ./public/test",
    "shadow:watch": "npx shadow-cljs watch app test",
    "shadow:build": "npx shadow-cljs release app",
    "test:build": "npx shadow-cljs compile ci",
    "test:run": "npx karma start karma.config.js --single-run",
    "clean": "npm run assets:clean",
    "repl": "npm run shadow:watch",
    "test": "npm run test:build && npm run test:run",
    "build": "npm run shadow:build",
    "dist": "rm -rf ./dist && mkdir -p ./dist && cp -R ./public/* ./dist",
    "dist:serve": "npx http-server ./dist -p 3000 --cors",
    "outdated": "clojure -M:nop:outdated && npx ncu",
    "outdated:upgrade": "clojure -M:nop:outdated:outdated/upgrade && npx ncu -u && npm ci",
    "setup": "npm ci"
  },
  "devDependencies": {
    "concurrently": "^6.5.0",
    "highlight.js": "11.3.1",
    "http-server": "^14.0.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cljs-test": "^0.1.0",
    "npm-check-updates": "^12.0.5",
    "prettier": "^2.5.1",
    "shadow-cljs": "^2.16.8"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
